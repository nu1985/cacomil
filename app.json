    },
    "ios": {
      "bundleIdentifier": "com.cacomil.CACOMIL",      
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "ca.comil.com",
      "versionCode": 2   
    }
  }
}
